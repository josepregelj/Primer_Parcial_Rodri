<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="utn.rodrigo_anton.primerparcial">

    <application
        android:allowBackup="true"
        android:icon="@drawable/splash_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activityes.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activityes.LogIn" />
        <activity android:name=".activityes.CarSelection" />
        <activity android:name=".activityes.Preferences" />
        <activity android:name=".activityes.CarInfo" />
        <activity android:name=".activityes.UserManager"></activity>
    </application>

</manifest>